{
  "name": "webpack3-boilerplate",
  "version": "1.0.0",
  "license": "ISC",
  "author": "Khoa Tran",
  "repository": {
    "type": "git",
    "url": "git://github.com/kuraby1389/WEB_StetheeCentral.git"
  },
  "jest": {
    "transform": {
      "^.+\\.(jsx|js)?$": "<rootDir>/node_modules/babel-jest"
    },
    "coverageDirectory": "<rootDir>/src/app/test/",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!src/app/components/sharedComponents/mediaPlayer/*.js",
      "!src/app/components/sharedComponents/mediaPlayer/**/*.js",
      "!src/app/components/sharedComponents/buttons/**/*.js",
      "!src/app/components/sharedComponents/AudioPlayer/**/*.js",
      "!src/app/test/**",
      "!src/app/*.js",
      "!src/app/components/sharedComponents/forms/_searchBar/*.js",
      "!src/app/components/protected/_myProfile/subscription_profile/card_section/newCard_dialog/*.js",
      "!webpack.config.js",
      "!gcb_slack.js",
      "!src/app/models/_interactor/*.js",
      "!index.js"
    ],
    "setupFiles": [
      "<rootDir>/src/app/test/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(svg)$": "<rootDir>/src/app/test/svgMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "dev": "webpack-dev-server",
    "clean": "rm -rf stage && mkdir stage",
    "build-stage": "npm run clean && webpack -p",
    "serve-stage": "PORT=9090 SERVER=DEV node index.js",
    "stage": "npm run build-stage && npm run serve-stage",
    "production": "sudo node index.js",
    "test": "jest",
    "test:coverage": "jest --coverage --silent",
    "test:clear": "jest -u",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@slack/client": "^4.5.0",
    "@types/jest": "^23.3.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.1"
  },
